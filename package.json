{
    "name": "rollup-plugin-copy-enhanced",
    "version": "1.1.2",
    "description": "Rollup plugin to copy and minify files during build.",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ouweiya/rollup-plugin-copy-enhanced"
    },
    "scripts": {
        "watch": "rimraf dist && tsc --watch",
        "tsc": "rimraf dist && tsc"
    },
    "keywords": [
        "rollup",
        "plugin",
        "copy",
        "minify",
        "build",
        "files",
        "assets",
        "html",
        "css",
        "json",
        "optimization"
    ],
    "files": [
        "!.github",
        "!deprecated",
        "dist",
        "tsconfig.json",
        "src"
    ],
    "author": "ouweiya",
    "license": "MIT",
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-typescript": "^11.1.5",
        "@rollup/pluginutils": "^5.1.0",
        "@tsconfig/node18-strictest-esm": "^1.0.1",
        "@types/node": "^20.10.6",
        "rimraf": "^5.0.5",
        "rollup": "^4.9.2",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@types/html-minifier-terser": "^7.0.2",
        "glob": "^10.3.10",
        "handlebars": "^4.7.8",
        "html-minifier-terser": "^7.2.0"
    }
}
